<div class="div-contact container-contact">

  <h2 class= "contact-talk">Let's talk</h2>

  <form class="div-contact form-contact">
    <div class="div-contact">
      <label for="name">Name*</label>
      <input  id="name" type="text" name="name" required>
    </div>
    <div class="div-contact">
      <label for="email">Email*</label>
      <input id="email" type="email" name="email" required>
    </div>
    <div class="div-contact">
      <label for="message">What can we help you with?*</label>
      <textarea id="message" name="message" required></textarea>
  </div>
    <button class="button contact" type="submit">Send</button>
  </form>
</div>
